<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample.keepfamily">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/familyicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChildActivity"></activity>
        <activity android:name=".MainActivity2" />
        <activity android:name=".fragment.RecordFragment" />
        <activity android:name=".fragment.Markfragment" />
        <activity android:name=".fragment.NaverMapFragment" />
        <activity android:name=".ChiSignUpActivity" />
        <activity android:name=".ChiLoginActivity" />
        <activity android:name=".ParSignUpActivity" />
        <activity android:name=".ParLoginActivity" />
        <activity android:name=".WelcomeActivity" />

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="hdc57i7t9q" />

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>